{
  "storage": {
    "rules": "storage.rules",
    "cors": [
      {
        "origin": ["*"],
        "method": ["GET", "POST", "PUT", "DELETE", "HEAD", "OPTIONS"],
        "maxAgeSeconds": 3600,
        "responseHeader": [
          "Content-Type",
          "Content-Length",
          "Content-Encoding",
          "Content-Disposition",
          "Cache-Control",
          "Cross-Origin-Resource-Policy"
        ]
      }
    ]
  }
}